@{
    ViewData["Title"] = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";

    var data = new List<dynamic>
    {
        new { id = 1, name = "Nguyen Van A", age = 25, country = "Vietnam" },
        new { id = 2, name = "John Smith", age = 30, country = "USA" },
        new { id = 3, name = "Sato Haruki", age = 22, country = "Japan" },
        new { id = 4, name = "Marie Dupont", age = 28, country = "France" },
        new { id = 5, name = "Hans Müller", age = 35, country = "Germany" },
        new { id = 6, name = "Emily Brown", age = 19, country = "Canada" },
        new { id = 7, name = "James Wilson", age = 27, country = "Australia" },
        new { id = 8, name = "Oliver Taylor", age = 40, country = "UK" },
        new { id = 9, name = "Kim Min-Jun", age = 32, country = "South Korea" },
        new { id = 10, name = "Amit Sharma", age = 29, country = "India" }
    };
}

<table border="1" cellpadding="8" cellspacing="0">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Age</th>
            <th>Country</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in data)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.name</td>
                <td>@item.age</td>
                <td>@item.country</td>
            </tr>
        }
    </tbody>
</table>
 